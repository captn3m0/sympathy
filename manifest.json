{
  "name": "Sympathy - Local Text Editor Inside Chrome",
  "version": "0.0.1",
  "incognito": "split",
  "icons": { 
    "128": "icons/icon_128.png",
    "16": "icons/icon_16.png"
  },
  "app": {
    "launch": {
      "local_path": "resources/application.html"
    }
  },
  "permissions": [
	"tabs",
    "notifications",
    "http://*.dropbox.com/",
    "https://*.dropbox.com/"
  ],
  "manifest_version":2,
   "plugins": [{"path": "plugin/npapi-file-io-32.dll"},{"path": "plugin/npapi-file-io-32.so"},{"path": "plugin/npapi-file-io-64.so"}]
}